@model DBAudit.Infrastructure.Contracts.Entities.ReportView


@{
    Layout = "_Layout";
}

<div class="container">
    <div class="row">
        <div class="col-12">
            @Model.Title
        </div>
        <div class="col-12">
            <div class="row">
                <div class="col-3">
                    <ul class="nav flex-column">
                        @foreach (var link in Model.Links)
                        {
                            <li class="nav-item">
                                <a class="nav-link" href="@link.Link">@link.Title</a>
                            </li>
                        }
                    </ul>

                </div>
                <div class="col-9">

                </div>
            </div>
        </div>
    </div>
</div>